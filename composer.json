{
    "name": "maxshdev/inscripcion",
    "type": "symfony-bundle",
    "description": "This bundle inscripcion generator",
    "keywords": ["inscripcion", "bootstrap", "admin", "generator"],
    "homepage": "https://github.com/maxshdev/InscripcionBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Shtefec Max",
            "email": "max.shtefec@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "twig/extensions": "^1.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "maxshdev\\Bundle\\InscripcionBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-version30": "4.0.x-dev"
        }
    }
}
